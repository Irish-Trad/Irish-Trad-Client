{
  "name": "irish-trad.net",
  "private": true,
  "description": "Irish-Trad.net provides free online resources and lessons for a variety of stringed instruments commonly found in traditional Irish sessions such as the Irish Bouzouki, Mandolin, Octave Mandola and Irish Tenor Banjo. ",
  "scripts": {
    "build": "next build",
    "check-packages": "npx npm-check-updates",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "dev": "next",
    "download-locales": "locize download --project-id=0842ada9-1d1d-4d48-ab27-08f6a132f558 --ver=latest --clean=true --path=./public/locales",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "lint": "ts-standard | snazzy",
    "lint-fix": "ts-standard --fix | snazzy",
    "sort-packages": "npx sort-package-json",
    "start": "next start",
    "test": "jest",
    "test:ci": "jest --ci"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "next": "^12.3.1",
    "next-i18next": "12.1.0",
    "next-language-detector": "1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.11.3",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.12",
    "babel-jest": "^29.2.1",
    "cypress": "^10.10.0",
    "jest": "^29.2.1",
    "locize-cli": "7.12.7",
    "postcss": "^8.4.18",
    "snazzy": "^9.0.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.1",
    "ts-standard": "^12.0.1",
    "typescript": "^4.8.4"
  },
  "ts-standard": {
    "ignore": [
      "**/*.d.ts",
      "cypress"
    ],
    "project": "./tsconfig.json"
  }
}
